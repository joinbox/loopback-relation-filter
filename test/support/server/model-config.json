{
    "_meta": {
        "sources": [
            "loopback/common/models",
            "loopback/server/models",
            "./models"
        ],
        "mixins": [
            "loopback/common/mixins",
            "loopback/server/mixins"
        ]
    },
    "Book": {
        "dataSource": "db"
    },
    "Page": {
        "dataSource": "db"
    },
    "Author": {
        "dataSource": "db"
    },
    "Publisher": {
        "dataSource": "db",
        "options": {
            "relationFilter":{
                "enabled": true
            }
        }
    },
    "UnsupportedModel": {
        "dataSource": "memory",
        "options": {
            "relationFilter": {
                "enabled": true
            }
        }
    }
}